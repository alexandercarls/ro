{
  "name": "ro2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "dotenv -- drizzle-kit migrate",
    "db:seed": "dotenv -- tsx database/seed.ts",
    "dev:ui": "react-router dev",
    "dev:zero": "zero-cache-dev -p ./app/schema.ts",
    "dev:db-up": "docker compose --env-file .env -f ./docker/docker-compose.yml up",
    "dev:db-down": "docker compose --env-file .env -f ./docker/docker-compose.yml down -v",
    "dev:clean": "source .env && docker volume rm -f docker_zstart_pgdata_1 && rm -rf \"${ZERO_REPLICA_FILE}\"*",
    "format": "prettier --write .  \"!**/*.json\"",
    "check:unused": "dotenv -- knip --max-issues 1",
    "check:unused:fix": "knip --fix",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint ."
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@react-router/express": "^7.1.1",
    "@react-router/fs-routes": "^7.1.1",
    "@react-router/node": "^7.1.1",
    "@react-router/serve": "^7.1.1",
    "@rocicorp/zero": "0.10.2024122404",
    "@xyflow/react": "^12.3.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "~0.38.3",
    "eslint-plugin-react-refresh": "^0.4.16",
    "isbot": "^5",
    "lucide-react": "^0.469.0",
    "postgres": "^3.4.5",
    "posthog-js": "^1.203.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-router": "^7.1.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@react-router/dev": "^7.1.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^19.0.0-beta-b2e8e9c-20241220",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "~0.30.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "knip": "^5.41.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-babel": "^1.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
